      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_CPdpox5fEeqX55Pn94gzHg" name="deplacementValide" specification="_CDGooB5fEeqX55Pn94gzHg">
        <ownedParameter xmi:id="_CPdppB5fEeqX55Pn94gzHg" name="direction">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#char"/>
        </ownedParameter>
        <ownedParameter xmi:id="_CPdppR5fEeqX55Pn94gzHg" name="xPos">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
        </ownedParameter>
        <ownedParameter xmi:id="_CPdpph5fEeqX55Pn94gzHg" name="yPos">
