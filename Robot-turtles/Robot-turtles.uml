            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_CPhUBB5fEeqX55Pn94gzHg" name="drawBoard" specification="_CDaxsx5fEeqX55Pn94gzHg">
              <language>java</language>
              <body>removeAll();&#xD;
		for (final TilePanel tilePanelRow[] : boardTiles) {&#xD;
			for (final TilePanel tilePanel : tilePanelRow) {&#xD;
				tilePanel.drawTile();&#xD;
				add(tilePanel);&#xD;
			}&#xD;
			&#xD;
			validate();&#xD;
			repaint();&#xD;
			&#xD;
		}&#xD;
		&#xD;
		&#xD;
	</body>
