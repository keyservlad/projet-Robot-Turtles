        <ownedParameter xmi:id="_CPdpqB5fEeqX55Pn94gzHg" direction="return">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
        </ownedParameter>
        <language>java</language>
        <body>switch(direction) {&#xD;
		case 'S':&#xD;
			if (yPos + 1 > 7) {&#xD;
				return false;&#xD;
			}&#xD;
			if (this.plateau[yPos + 1][xPos] == 'X') {&#xD;
				Game.victoire(color);&#xD;
			}&#xD;
			else if ((this.plateau[yPos + 1][xPos] != ' ')) {&#xD;
				return false;&#xD;
			}&#xD;
			&#xD;
			&#xD;
			&#xD;
			&#xD;
			break;&#xD;
			&#xD;
		case 'N':&#xD;
			if (yPos - 1 &lt; 0) {&#xD;
				return false;&#xD;
			}&#xD;
			if (this.plateau[yPos - 1][xPos] == 'X') {&#xD;
				Game.victoire(color);&#xD;
			}&#xD;
			else if (this.plateau[yPos - 1][xPos] != ' ') {&#xD;
				return false;&#xD;
			}&#xD;
			&#xD;
			&#xD;
			break;&#xD;
			&#xD;
		case 'E':&#xD;
			if (xPos > 7) {&#xD;
				return false;&#xD;
			}&#xD;
			if (this.plateau[yPos][xPos + 1] == 'X') {&#xD;
				Game.victoire(color);&#xD;
			}&#xD;
			else if (this.plateau[yPos][xPos + 1] != ' ') {&#xD;
				return false;&#xD;
			}&#xD;
			&#xD;
			&#xD;
			break;&#xD;
			&#xD;
		case 'W':&#xD;
			if (xPos - 1 &lt; 0) {&#xD;
				return false;&#xD;
			}&#xD;
			if (this.plateau[yPos][xPos - 1] == 'X') {&#xD;
				Game.victoire(color);&#xD;
			}&#xD;
			else if (this.plateau[yPos][xPos - 1] != ' ') {&#xD;
				return false;&#xD;
			}&#xD;
			&#xD;
			break;&#xD;
			&#xD;
		}&#xD;
		&#xD;
		&#xD;
		&#xD;
		&#xD;
		return true;&#xD;
	</body>
      </ownedBehavior>
