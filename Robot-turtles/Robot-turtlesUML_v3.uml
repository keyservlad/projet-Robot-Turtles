          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_hPoKgzu1Eeqfo67y6Udy8w" name="deplacementValide" specification="_g73hADu1Eeqfo67y6Udy8w">
            <ownedParameter xmi:id="_hPoKhDu1Eeqfo67y6Udy8w" name="direction">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#char"/>
            </ownedParameter>
            <ownedParameter xmi:id="_hPoKhTu1Eeqfo67y6Udy8w" name="xPos">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
            </ownedParameter>
            <ownedParameter xmi:id="_hPoKhju1Eeqfo67y6Udy8w" name="yPos">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
            </ownedParameter>
            <ownedParameter xmi:id="_hPoKhzu1Eeqfo67y6Udy8w" name="color">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
            </ownedParameter>
            <ownedParameter xmi:id="_hPoKiDu1Eeqfo67y6Udy8w" direction="return">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
            </ownedParameter>
            <language>java</language>
            <body>switch (direction) {&#xD;
		case 'S':&#xD;
			if (yPos + 1 > 7) {&#xD;
				return false;&#xD;
			}&#xD;
			if (this.plateau[yPos + 1][xPos] == 'X') {&#xD;
				Game.victoire(color);&#xD;
			} else if ((this.plateau[yPos + 1][xPos] == 'R') || (this.plateau[yPos + 1][xPos] == 'V') || (this.plateau[yPos + 1][xPos] == 'B') || (this.plateau[yPos + 1][xPos] == 'J')) {&#xD;
				for (Tortue tortue : Game.tortues) {&#xD;
					if (tortue.getSymbol() == plateau[yPos + 1][xPos]) {&#xD;
						Game.resetTortue(tortue);&#xD;
					}&#xD;
					if (tortue.getSymbol() == plateau[yPos][xPos]) {&#xD;
						Game.resetTortue(tortue);&#xD;
					}&#xD;
				}&#xD;
				return false;&#xD;
			}else if ((this.plateau[yPos + 1][xPos] != ' ')) {&#xD;
				return false;&#xD;
			}&#xD;
&#xD;
			break;&#xD;
&#xD;
		case 'N':&#xD;
			if (yPos - 1 &lt; 0) {&#xD;
				return false;&#xD;
			}&#xD;
			if (this.plateau[yPos - 1][xPos] == 'X') {&#xD;
				Game.victoire(color);&#xD;
			} else if ((this.plateau[yPos - 1][xPos] == 'R') || (this.plateau[yPos - 1][xPos] == 'V') || (this.plateau[yPos - 1][xPos] == 'B') || (this.plateau[yPos - 1][xPos] == 'J')) {&#xD;
				for (Tortue tortue : Game.tortues) {&#xD;
					if (tortue.getSymbol() == plateau[yPos - 1][xPos]) {&#xD;
						Game.resetTortue(tortue);&#xD;
					}&#xD;
					if (tortue.getSymbol() == plateau[yPos][xPos]) {&#xD;
						Game.resetTortue(tortue);&#xD;
					}&#xD;
				}&#xD;
				return false;&#xD;
			}else if(this.plateau[yPos - 1][xPos] != ' ') {&#xD;
				return false;&#xD;
			}&#xD;
&#xD;
			break;&#xD;
&#xD;
		case 'E':&#xD;
			if (xPos > 7) {&#xD;
				return false;&#xD;
			}&#xD;
			if (this.plateau[yPos][xPos + 1] == 'X') {&#xD;
				Game.victoire(color);&#xD;
			} else if ((this.plateau[yPos][xPos + 1] == 'R') || (this.plateau[yPos][xPos + 1] == 'V') || (this.plateau[yPos][xPos + 1] == 'B') || (this.plateau[yPos][xPos + 1] == 'J')) {&#xD;
				for (Tortue tortue : Game.tortues) {&#xD;
					if (tortue.getSymbol() == plateau[yPos][xPos + 1]) {&#xD;
						Game.resetTortue(tortue);&#xD;
					}&#xD;
					if (tortue.getSymbol() == plateau[yPos][xPos]) {&#xD;
						Game.resetTortue(tortue);&#xD;
					}&#xD;
				}&#xD;
				return false;&#xD;
			}else if (this.plateau[yPos][xPos + 1] != ' ') {&#xD;
				return false;&#xD;
			}&#xD;
&#xD;
			break;&#xD;
&#xD;
		case 'W':&#xD;
			if (xPos - 1 &lt; 0) {&#xD;
				return false;&#xD;
			}&#xD;
			if (this.plateau[yPos][xPos - 1] == 'X') {&#xD;
				Game.victoire(color);&#xD;
			} else if ( (this.plateau[yPos][xPos - 1] == 'R') || (this.plateau[yPos][xPos - 1] == 'V') || (this.plateau[yPos][xPos - 1] == 'B') || (this.plateau[yPos][xPos - 1] == 'J')) {&#xD;
				for (Tortue tortue : Game.tortues) {&#xD;
					if (tortue.getSymbol() == plateau[yPos][xPos - 1]) {&#xD;
						Game.resetTortue(tortue);&#xD;
					}&#xD;
					if (tortue.getSymbol() == plateau[yPos][xPos]) {&#xD;
						Game.resetTortue(tortue);&#xD;
					}&#xD;
				}&#xD;
				return false;&#xD;
			}else if (this.plateau[yPos][xPos - 1] != ' ') {&#xD;
				return false;&#xD;
			}&#xD;
&#xD;
			break;&#xD;
&#xD;
		}&#xD;
&#xD;
		return true;&#xD;
	</body>
          </ownedBehavior>
