            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_hPyikDu1Eeqfo67y6Udy8w" name="drawBoard" specification="_g8elADu1Eeqfo67y6Udy8w">
              <language>java</language>
              <body>removeAll();&#xD;
		for (final TilePanel tilePanelRow[] : boardTiles) {&#xD;
			for (final TilePanel tilePanel : tilePanelRow) {&#xD;
				tilePanel.drawTile();&#xD;
				add(tilePanel);&#xD;
			}&#xD;
			&#xD;
			validate();&#xD;
			repaint();&#xD;
			&#xD;
		}&#xD;
		&#xD;
		&#xD;
	</body>
